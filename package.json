    "rollup-plugin-terser": "^5.1.1",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.3",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.8.0"
  },
  "peerDependencies": {
    "lit-element": "2.x.x",
    "lit-html": "1.x.x",
    "pwa-helpers": "0.x.x",
    "redux": "4.x.x"
  },
  "bundlesize": [
    {
      "path": "dist/lit-redux-router.min.js",
      "maxSize": "1.6 kB"
    }
  ]
}
