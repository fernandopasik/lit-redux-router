    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.15.1",
    "eslint-plugin-lit": "^1.2.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.4",
    "jest": "^24.9.0",
    "jest-junit": "^7.0.0",
    "lint-staged": "^9.2.3",
    "lit-analyzer": "^1.1.8",
    "lit-element": "^2.1.0",
    "lit-html": "^1.1.0",
    "pwa-helpers": "^0.9.1",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.8",
    "redux-mock-store": "^1.5.3",
    "rollup": "^1.19.4",
